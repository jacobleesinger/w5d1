<h1>Sign In</h1>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>

  <label>Username
    <input type="text" id="username" name="user[username]" value="">
  </label>
  <br>
  <label>Password
    <input type="password" id="password" name="user[password]" value="">
  </label>
  <br>
  <input type="submit" value="Submit">
</form>
