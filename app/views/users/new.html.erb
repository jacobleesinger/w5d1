<h1>Create User</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label>Username
    <input type="text" id="username" name="user[username]" value="<%= @user.username %>">
  </label>
  <br>
  <label>Password
    <input type="password" id="password" name="user[password]" value="">
  </label>
  <br>
  <input type="submit" value="Submit">
</form>
